{
  "expo": {
    "name": "heyWattsApp",
    "slug": "heyWattsApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/branding/hw-iso-hor-color-01.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/branding/hw-iso-hor-color-01.png",
      "resizeMode": "contain",
      "backgroundColor": "#2A434D"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/branding/hw-iso-hor-color-01.png",
        "backgroundColor": "#2A434D"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
